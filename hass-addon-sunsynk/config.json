{
  "name": "Sunsynk Inverter Add-on",
  "slug": "hass-addon-sunsynk",
  "description": "Add-on for the Sunsynk Inverter",
  "version": "2021.9.18-0.0.4",
  "startup": "services",
  "boot": "auto",
  "url": "https://github.com/kellerza/sunsynk",
  "uart": true,
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "ports": {
  },
  "ports_description": {
  },
  "host_network": true,
  "options": {
    "PORT": "/dev/ttyUSB0",
    "SUNSYNK_ID": "my_ss_1",
    "SENSORS": ["total_pv_power","total_battery_charge", "total_battery_discharge", "total_grid_export", "total_grid_import", "battery_soc"],
    "MQTT_HOST": "core-mosquitto",
    "MQTT_PORT": 1883,
    "MQTT_USERNAME": "hass",
    "MQTT_PASSWORD": "",
    "DEBUG": 0
  },
  "schema": {
    "PORT": "device(subsystem=tty)",
    "SUNSYNK_ID": "str",
    "SENSORS": ["str"],
    "MQTT_HOST": "str",
    "MQTT_PORT": "port",
    "MQTT_USERNAME": "str",
    "MQTT_PASSWORD": "password",
    "DEBUG": "bool"
  }
}
